#include<iostream>
using namespace std;
 int main()
   { 
     int integertype;
     char charactertype;
	   float floattype;
	   double doubletype;
   cout<<"size of integer type is"<<sizeof(int)<<endl;
   cout<<"size of character type is"<<sizeof(char)<<endl;
   cout<<"size of float type is"<<sizeof(float)<<endl;
   cout<<"size of double type is"<<sizeof(double);
      return 0;
   }
